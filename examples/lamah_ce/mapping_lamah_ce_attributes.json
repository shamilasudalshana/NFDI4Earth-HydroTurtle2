    ],

    "lc_dom": [
      ["@subject", "@catchment"],
      ["rdf:type", "envthes:30212"],
      ["hyobs:hasDominantLandUse", "^^xsd:decimal"]
    ],

    "agr_fra": [
      ["@subject", "@catchment"],
      ["rdf:type", "envthes:30212"],
      ["hyobs:hasAgriculturalAreaFraction", "^^xsd:decimal"]
    ],

    "bare_fra": [
      ["@subject", "@catchment"],
      ["rdf:type", "envthes:30212"],
      ["hyobs:hasBareAreaFraction", "^^xsd:decimal"]
    ],

    "forest_fra": [
      ["@subject", "@catchment"],
      ["rdf:type", "envthes:30212"],
