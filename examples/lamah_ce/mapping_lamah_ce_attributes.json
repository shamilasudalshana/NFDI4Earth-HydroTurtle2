    "lo_prec_du": [
      ["@subject", "@catchment"],
      ["rdf:type", "envthes:30212"],
      ["hyobs:hasLowPrecipitationDuration", [
        ["rdf:type","qudt:QuantityValue"],
        ["qudt:numericValue","^^xsd:decimal"],
